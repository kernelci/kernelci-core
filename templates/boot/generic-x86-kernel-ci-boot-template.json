{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "metadata": {
                "image.type": "{image_type}",
                "image.url": "{image_url}",
                "kernel.tree": "{tree}",
                "kernel.describe": "{kernel_describe}",
                "kernel.branch": "{kernel_branch}",
                "arch": "{arch}",
                "kernel.endian": "{endian}",
                "kernel.defconfig": "{defconfig}",
                "platform.fastboot": "{fastboot}",
                "test.plan": "{test_plan}"
            },
            "parameters": {
                "bootloadertype": "ipxe",
                "kernel": "{kernel_url}",
                "overlays": ["{modules_url}"],
                "ramdisk": "http://storage.kernelci.org/images/rootfs/buildroot/x86/rootfs.cpio.gz"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "{device_type}",
    "job_name": "{job_name}",
    "logging_level": "DEBUG",
    "priority": "{priority}",
    "timeout": 600
}